      <bind nodeset="/fs_meg_situation/user_id" type="string" calculate="../inputs/user/contact_id"/>
      <bind nodeset="/fs_meg_situation/user_phone" type="string" calculate="../inputs/user/phone"/>
      <bind nodeset="/fs_meg_situation/patient_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/fs_meg_situation/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/fs_meg_situation/hospital_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/fs_meg_situation/commune_id" type="string" calculate="../inputs/contact/parent/_id"/>
      <bind nodeset="/fs_meg_situation/prefecture_id" type="string" calculate="../inputs/contact/parent/parent/_id"/>
      <bind nodeset="/fs_meg_situation/region_id" type="string" calculate="../inputs/contact/parent/parent/parent/_id"/>
      <bind nodeset="/fs_meg_situation/country_id" type="string" calculate="../inputs/contact/parent/parent/parent/parent/_id"/>
      <bind nodeset="/fs_meg_situation/meg_average_group/action_date" type="date" constraint=". &lt;= today()" jr:requiredMsg="jr:itext('/fs_meg_situation/meg_average_group/action_date:jr:requiredMsg')" required="true()" jr:constraintMsg="jr:itext('/fs_meg_situation/meg_average_group/action_date:jr:constraintMsg')"/>
      <bind nodeset="/fs_meg_situation/meg_average_group/month_date_selected" type="select1" calculate="format-date-time( decimal-date-time(../action_date), &quot;%m&quot; )" required="true()" readonly="true()"/>
      <bind nodeset="/fs_meg_situation/meg_average_group/month_day" type="int" calculate="int( jr:choice-name( /fs_meg_situation/meg_average_group/month_date_selected ,' /fs_meg_situation/meg_average_group/month_date_selected ') )" relevant=" /fs_meg_situation/meg_average_group/action_date  != ''" required="true()" readonly="true()"/>
      <bind nodeset="/fs_meg_situation/meg_average_group/all_med_shortage_days_number" type="int" constraint=". &gt; 0" jr:requiredMsg="jr:itext('/fs_meg_situation/meg_average_group/all_med_shortage_days_number:jr:requiredMsg')" required="true()" jr:constraintMsg="jr:itext('/fs_meg_situation/meg_average_group/all_med_shortage_days_number:jr:constraintMsg')"/>
      <bind nodeset="/fs_meg_situation/meg_average_group/all_med_number" type="int" constraint=". &gt; 0" jr:requiredMsg="jr:itext('/fs_meg_situation/meg_average_group/all_med_number:jr:requiredMsg')" required="true()" jr:constraintMsg="jr:itext('/fs_meg_situation/meg_average_group/all_med_number:jr:constraintMsg')"/>
      <bind nodeset="/fs_meg_situation/meg_average_group/meg_average_out_of" type="string" calculate="round((( /fs_meg_situation/meg_average_group/month_day  *  /fs_meg_situation/meg_average_group/all_med_number ) -  /fs_meg_situation/meg_average_group/all_med_shortage_days_number ) div ( /fs_meg_situation/meg_average_group/month_day  *  /fs_meg_situation/meg_average_group/all_med_number ), 2) * 100"/>
      <bind nodeset="/fs_meg_situation/meg_average_group/meg_average_available" type="string" calculate="100 -  /fs_meg_situation/meg_average_group/meg_average_out_of "/>
      <bind nodeset="/fs_meg_situation/meg_average_group/shortage_days_average" readonly="true()" type="string" relevant=" /fs_meg_situation/meg_average_group/meg_average_out_of  &gt;= 0"/>
      <bind nodeset="/fs_meg_situation/meg_average_group/available_days_average" readonly="true()" type="string" relevant=" /fs_meg_situation/meg_average_group/meg_average_available  &gt;= 0"/>
      <bind nodeset="/fs_meg_situation/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
