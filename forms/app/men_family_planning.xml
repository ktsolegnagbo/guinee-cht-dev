      <input ref="/men_family_planning/group_review/s_condoms_given">
        <label ref="jr:itext('/men_family_planning/group_review/s_condoms_given:label')"/>
      </input>
      <input appearance="h1 green" ref="/men_family_planning/group_review/followup">
        <label ref="jr:itext('/men_family_planning/group_review/followup:label')"/>
