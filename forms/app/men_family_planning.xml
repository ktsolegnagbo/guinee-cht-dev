            <label ref="jr:itext('/men_family_planning/inputs/contact/parent/_id:label')"/>
          </input>
          <input ref="/men_family_planning/inputs/contact/parent/name">
            <label ref="jr:itext('/men_family_planning/inputs/contact/parent/name:label')"/>
          </input>
          <input ref="/men_family_planning/inputs/contact/parent/external_id">
            <label ref="jr:itext('/men_family_planning/inputs/contact/parent/external_id:label')"/>
          </input>
          <group ref="/men_family_planning/inputs/contact/parent/parent">
            <label ref="jr:itext('/men_family_planning/inputs/contact/parent/parent:label')"/>
            <input appearance="select-contact type-person" ref="/men_family_planning/inputs/contact/parent/parent/_id">
              <label ref="jr:itext('/men_family_planning/inputs/contact/parent/parent/_id:label')"/>
            </input>
            <group ref="/men_family_planning/inputs/contact/parent/parent/parent">
              <label ref="jr:itext('/men_family_planning/inputs/contact/parent/parent/parent:label')"/>
              <input ref="/men_family_planning/inputs/contact/parent/parent/parent/_id">
                <label ref="jr:itext('/men_family_planning/inputs/contact/parent/parent/parent/_id:label')"/>
              </input>
              <group ref="/men_family_planning/inputs/contact/parent/parent/parent/parent">
                <label ref="jr:itext('/men_family_planning/inputs/contact/parent/parent/parent/parent:label')"/>
                <input ref="/men_family_planning/inputs/contact/parent/parent/parent/parent/_id">
                  <label ref="jr:itext('/men_family_planning/inputs/contact/parent/parent/parent/parent/_id:label')"/>
                </input>
                <group ref="/men_family_planning/inputs/contact/parent/parent/parent/parent/parent">
                  <label ref="jr:itext('/men_family_planning/inputs/contact/parent/parent/parent/parent/parent:label')"/>
                  <input ref="/men_family_planning/inputs/contact/parent/parent/parent/parent/parent/_id">
                    <label ref="jr:itext('/men_family_planning/inputs/contact/parent/parent/parent/parent/parent/_id:label')"/>
                  </input>
                  <group ref="/men_family_planning/inputs/contact/parent/parent/parent/parent/parent/parent">
                    <label ref="jr:itext('/men_family_planning/inputs/contact/parent/parent/parent/parent/parent/parent:label')"/>
                    <input ref="/men_family_planning/inputs/contact/parent/parent/parent/parent/parent/parent/_id">
                      <label ref="jr:itext('/men_family_planning/inputs/contact/parent/parent/parent/parent/parent/parent/_id:label')"/>
                    </input>
                    <group ref="/men_family_planning/inputs/contact/parent/parent/parent/parent/parent/parent/parent">
                      <label ref="jr:itext('/men_family_planning/inputs/contact/parent/parent/parent/parent/parent/parent/parent:label')"/>
                      <input ref="/men_family_planning/inputs/contact/parent/parent/parent/parent/parent/parent/parent/_id">
                        <label ref="jr:itext('/men_family_planning/inputs/contact/parent/parent/parent/parent/parent/parent/parent/_id:label')"/>
                      </input>
                      <group ref="/men_family_planning/inputs/contact/parent/parent/parent/parent/parent/parent/parent/parent">
                        <label ref="jr:itext('/men_family_planning/inputs/contact/parent/parent/parent/parent/parent/parent/parent/parent:label')"/>
                        <input ref="/men_family_planning/inputs/contact/parent/parent/parent/parent/parent/parent/parent/parent/_id">
                          <label ref="jr:itext('/men_family_planning/inputs/contact/parent/parent/parent/parent/parent/parent/parent/parent/_id:label')"/>
                        </input>
                      </group>
                    </group>
                  </group>
                </group>
