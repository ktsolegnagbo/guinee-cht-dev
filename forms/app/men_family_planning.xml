      <bind nodeset="/men_family_planning/patient_age_in_years" type="string" calculate="if (  /men_family_planning/inputs/contact/date_of_birth  = '', '', floor( difference-in-months(  /men_family_planning/inputs/contact/date_of_birth , today() ) div 12 ) )"/>
      <bind nodeset="/men_family_planning/patient_age_in_months" type="string" calculate="difference-in-months(  /men_family_planning/inputs/contact/date_of_birth , today() )"/>
      <bind nodeset="/men_family_planning/patient_age_in_days" type="string" calculate="floor( decimal-date-time(today()) - decimal-date-time( /men_family_planning/inputs/contact/date_of_birth ) )"/>
      <bind nodeset="/men_family_planning/patient_age_display_fr" type="string" calculate="concat(concat( /men_family_planning/patient_age_in_years , if( /men_family_planning/patient_age_in_years  =1,' an et ',' ans et ')),concat( /men_family_planning/patient_age_in_months  mod 12, if(( /men_family_planning/patient_age_in_months  mod 12)=1,' mois',' mois')))"/>
      <bind nodeset="/men_family_planning/patient_age_display_en" type="string" calculate="concat(concat( /men_family_planning/patient_age_in_years , if( /men_family_planning/patient_age_in_years  =1,' year and ',' years and ')),concat( /men_family_planning/patient_age_in_months  mod 12, if(( /men_family_planning/patient_age_in_months  mod 12)=1,' month old',' months old')))"/>
      <bind nodeset="/men_family_planning/patient_sex_en" type="string" calculate="if( /men_family_planning/inputs/contact/sex  = 'male' or  /men_family_planning/inputs/contact/sex  = 'homme' or  /men_family_planning/inputs/contact/sex  = 'Homme', 'Male', if( /men_family_planning/inputs/contact/sex  = 'female' or  /men_family_planning/inputs/contact/sex  = 'femme' or  /men_family_planning/inputs/contact/sex  = 'Femme', 'Female', ''))"/>
      <bind nodeset="/men_family_planning/patient_sex_fr" type="string" calculate="if( /men_family_planning/inputs/contact/sex  = 'male' or  /men_family_planning/inputs/contact/sex  = 'homme' or  /men_family_planning/inputs/contact/sex  = 'Homme', 'Masculin', if( /men_family_planning/inputs/contact/sex  = 'female' or  /men_family_planning/inputs/contact/sex  = 'femme' or  /men_family_planning/inputs/contact/sex  = 'Femme', 'Féminin', ''))"/>
      <bind nodeset="/men_family_planning/patient_family_name" type="string" calculate="../inputs/contact/parent/name"/>
      <bind nodeset="/men_family_planning/patient_family_external_id" type="string" calculate="../inputs/contact/parent/external_id"/>
      <bind nodeset="/men_family_planning/visited_contact_uuid" type="string" calculate="../inputs/contact/parent/_id"/>
      <bind nodeset="/men_family_planning/household_id" type="string" calculate="../inputs/contact/parent/_id"/>
      <bind nodeset="/men_family_planning/village_secteur_id" type="string" calculate="../inputs/contact/parent/parent/_id"/>
      <bind nodeset="/men_family_planning/district_quartier_id" type="string" calculate="../inputs/contact/parent/parent/parent/_id"/>
      <bind nodeset="/men_family_planning/hospital_id" type="string" calculate="../inputs/contact/parent/parent/parent/parent/_id"/>
      <bind nodeset="/men_family_planning/commune_id" type="string" calculate="../inputs/contact/parent/parent/parent/parent/parent/_id"/>
      <bind nodeset="/men_family_planning/prefecture_id" type="string" calculate="../inputs/contact/parent/parent/parent/parent/parent/parent/_id"/>
      <bind nodeset="/men_family_planning/region_id" type="string" calculate="../inputs/contact/parent/parent/parent/parent/parent/parent/parent/_id"/>
      <bind nodeset="/men_family_planning/country_id" type="string" calculate="../inputs/contact/parent/parent/parent/parent/parent/parent/parent/parent/_id"/>
      <bind nodeset="/men_family_planning/fp_method" type="string" calculate=" /men_family_planning/c_planning/c_method_wanted "/>
      <bind nodeset="/men_family_planning/fp_method_name" type="string" calculate="if( /men_family_planning/c_planning/c_method_wanted  = '','',jr:choice-name( /men_family_planning/c_planning/c_method_wanted ,' /men_family_planning/c_planning/c_method_wanted '))"/>
      <bind nodeset="/men_family_planning/condoms_quantity" type="string" calculate=" /men_family_planning/condoms_quantity "/>
      <bind nodeset="/men_family_planning/c_planning/c_method_wanted" type="select1" required="true()"/>
      <bind nodeset="/men_family_planning/c_planning/c_condoms_quantity" type="int" constraint=". &gt;= 0" relevant="selected( /men_family_planning/c_planning/c_method_wanted ,'condoms')" required="true()" jr:constraintMsg="jr:itext('/men_family_planning/c_planning/c_condoms_quantity:jr:constraintMsg')"/>
      <bind nodeset="/men_family_planning/group_review/s_summary" readonly="true()" type="string"/>
      <bind nodeset="/men_family_planning/group_review/s_patient_info_name" readonly="true()" type="string"/>
      <bind nodeset="/men_family_planning/group_review/s_patient_info_sex" readonly="true()" type="string"/>
      <bind nodeset="/men_family_planning/group_review/s_patient_info_age" readonly="true()" type="string"/>
      <bind nodeset="/men_family_planning/group_review/s_information_on_pf" readonly="true()" type="string"/>
      <bind nodeset="/men_family_planning/group_review/s_condoms_given" readonly="true()" type="string"/>
      <bind nodeset="/men_family_planning/group_review/followup" readonly="true()" type="string"/>
      <bind nodeset="/men_family_planning/group_review/s_no_followup" readonly="true()" type="string"/>
      <bind nodeset="/men_family_planning/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/men_family_planning/inputs">
      <label ref="jr:itext('/men_family_planning/inputs:label')"/>
