              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source>user</source>
            <source_id/>
            <user>
              <contact_id/>
              <name/>
              <phone/>
            </user>
            <contact>
              <_id/>
              <name/>
              <parent>
                <_id/>
                <parent>
                  <_id/>
                  <parent>
                    <_id/>
                    <parent>
                      <_id/>
                      <parent>
                        <_id/>
                        <parent>
                          <_id/>
                        </parent>
                      </parent>
                    </parent>
                  </parent>
                </parent>
              </parent>
            </contact>
          </inputs>
          <user_id/>
          <user_phone/>
          <patient_id/>
          <patient_name/>
          <village_secteur_id/>
          <district_quartier_id/>
          <hospital_id/>
          <commune_id/>
          <prefecture_id/>
          <region_id/>
          <country_id/>
          <event_desease>
            <events/>
            <other_event/>
            <event_name/>
            <is_event/>
            <is_illness/>
            <event_date/>
            <village_location_name/>
            <name/>
            <sex/>
            <age/>
            <name_person_in_charge/>
            <phone_person_in_charge/>
            <health_center_feedback_date/>
            <health_center_feedback_location/>
          </event_desease>
          <meta>
            <instanceID/>
          </meta>
        </event_register>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/event_register/inputs/source" type="string"/>
      <bind nodeset="/event_register/inputs/source_id" type="string"/>
      <bind nodeset="/event_register/inputs/user/contact_id" type="db:person" readonly="true()"/>
      <bind nodeset="/event_register/inputs/user/name" type="string"/>
      <bind nodeset="/event_register/inputs/user/phone" type="string"/>
      <bind nodeset="/event_register/inputs/contact/_id" type="string"/>
      <bind nodeset="/event_register/inputs/contact/name" type="string"/>
      <bind nodeset="/event_register/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/event_register/inputs/contact/parent/parent/_id" type="string"/>
      <bind nodeset="/event_register/inputs/contact/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/event_register/inputs/contact/parent/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/event_register/inputs/contact/parent/parent/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/event_register/inputs/contact/parent/parent/parent/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/event_register/user_id" type="string" calculate="../inputs/user/contact_id"/>
      <bind nodeset="/event_register/user_phone" type="string" calculate="../inputs/user/phone"/>
      <bind nodeset="/event_register/patient_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/event_register/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/event_register/village_secteur_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/event_register/district_quartier_id" type="string" calculate="../inputs/contact/parent/_id"/>
      <bind nodeset="/event_register/hospital_id" type="string" calculate="../inputs/contact/parent/parent/_id"/>
      <bind nodeset="/event_register/commune_id" type="string" calculate="../inputs/contact/parent/parent/parent/_id"/>
      <bind nodeset="/event_register/prefecture_id" type="string" calculate="../inputs/contact/parent/parent/parent/parent/_id"/>
      <bind nodeset="/event_register/region_id" type="string" calculate="../inputs/contact/parent/parent/parent/parent/parent/_id"/>
      <bind nodeset="/event_register/country_id" type="string" calculate="../inputs/contact/parent/parent/parent/parent/parent/parent/_id"/>
      <bind nodeset="/event_register/event_desease/events" type="select1" jr:requiredMsg="jr:itext('/event_register/event_desease/events:jr:requiredMsg')" required="true()"/>
      <bind nodeset="/event_register/event_desease/other_event" type="string" relevant="selected(../events,'others_event') or selected(../events,'others_illness')"/>
      <bind nodeset="/event_register/event_desease/event_name" type="string" calculate="if(../events = '','',if(../events = 'others',../other_event,jr:choice-name( /event_register/event_desease/events ,' /event_register/event_desease/events ')))"/>
      <bind nodeset="/event_register/event_desease/is_event" type="string" calculate="if(selected(../events,'flood') or selected(../events,'fire') or selected(../events,'shipwreck') or selected(../events,'landslide') or selected(../events,'grouped_animal_deaths') or selected(../events,'others_event') or selected(../events,'community_deaths'),'true','false')"/>
      <bind nodeset="/event_register/event_desease/is_illness" type="string" calculate="if(selected(../events,'influenza_fever') or selected(../events,'dermatosis') or selected(../events,'measles') or selected(../events,'gender_based_v') or selected(../events,'perinatal_deaths') or selected(../events,'pfa') or selected(../events,'bloody_diarrhea') or selected(../events,'yellow_fever') or selected(../events,'cholera') or selected(../events,'maternal_and_neonatal_tetanus') or selected(../events,'viral_diseases') or selected(../events,'meningitis') or selected(../events,'maternal_deaths') or selected(../events,'others_illness'),'true','false')"/>
      <bind nodeset="/event_register/event_desease/event_date" type="date" required="true()" jr:constraintMsg="jr:itext('/event_register/event_desease/event_date:jr:constraintMsg')" constraint=". &lt;= today()" jr:requiredMsg="jr:itext('/event_register/event_desease/event_date:jr:requiredMsg')"/>
      <bind nodeset="/event_register/event_desease/village_location_name" type="string" jr:requiredMsg="jr:itext('/event_register/event_desease/village_location_name:jr:requiredMsg')" required="true()"/>
      <bind nodeset="/event_register/event_desease/name" type="string" relevant="../is_illness = 'true'" required="true()"/>
      <bind nodeset="/event_register/event_desease/sex" type="select1" relevant="../is_illness = 'true'" required="true()"/>
      <bind nodeset="/event_register/event_desease/age" type="int" relevant="../is_illness = 'true'" required="true()"/>
      <bind nodeset="/event_register/event_desease/name_person_in_charge" type="string" relevant="../is_event = 'true'" required="true()"/>
      <bind nodeset="/event_register/event_desease/phone_person_in_charge" type="string" jr:constraintMsg="jr:itext('/event_register/event_desease/phone_person_in_charge:jr:constraintMsg')" constraint="regex(., '^\d{9}$')" relevant="../is_event = 'true'"/>
      <bind nodeset="/event_register/event_desease/health_center_feedback_date" type="date" jr:constraintMsg="jr:itext('/event_register/event_desease/health_center_feedback_date:jr:constraintMsg')" constraint=". &gt;=  /event_register/event_desease/event_date  and . &lt;= today()" calculate="today()" readonly="true()"/>
      <bind nodeset="/event_register/event_desease/health_center_feedback_location" type="select1" jr:requiredMsg="jr:itext('/event_register/event_desease/health_center_feedback_location:jr:requiredMsg')" required="true()"/>
      <bind nodeset="/event_register/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
